<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:GrabMarket.Views"
                    xmlns:vm="clr-namespace:GrabMarket.View_Models">

    <DataTemplate DataType="{x:Type vm:MenuViewModel}">
        <v:Menu/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ManagerViewModel}">
        <v:ManagerPanel/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CashierViewModel}">
        <v:CashierPanel/>
    </DataTemplate>
    
</ResourceDictionary>